{
  "name": "MixCat",
  "description": "A Discord bot that mixed Node.js with Vite, Vue, and Pinia",
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Proladon/startcord.git"
  },
  "keywords": [
    "discordjs",
    "discord",
    "bot",
    "vue",
    "vite"
  ],
  "author": "Proladon",
  "scripts": {
    "dev": "vite-node src/index.ts",
    "start": "cross-env MODE=production vite-node src/main.ts ",
    "docker:build": "cross-env MODE=production sudo ./script/build_docker.sh",
    "docker:run": "sudo ./script/run_docker.sh"
  },
  "dependencies": {
    "@discordjs/rest": "^2.6.0",
    "@hono/node-server": "^1.19.7",
    "axios": "^1.13.2",
    "cron": "^4.4.0",
    "dayjs": "^1.11.19",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.3",
    "git-rev-sync": "^3.0.2",
    "hono": "^4.10.8",
    "lodash-es": "^4.17.21",
    "lru-cache": "^11.2.4",
    "pinia": "^3.0.4",
    "vue": "^3.5.25"
  },
  "devDependencies": {
    "@types/git-rev-sync": "^2.0.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.0.0",
    "cross-env": "^10.1.0",
    "unplugin-auto-import": "^20.3.0",
    "vite-node": "^5.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}