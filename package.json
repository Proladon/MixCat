{
  "name": "MixCat",
  "description": "A Discord bot that mixed Node.js with Vite, Vue, and Pinia",
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Proladon/startcord.git"
  },
  "keywords": [
    "discord",
    "bot",
    "vue",
    "vite"
  ],
  "author": "Proladon",
  "scripts": {
    "dev": "cross-env MODE=development vite-node --watch src/main.ts ",
    "start": "cross-env MODE=production vite-node src/main.ts ",
    "docker:build": "cross-env MODE=production sudo ./script/build_docker.sh",
    "docker:run": "sudo ./script/run_docker.sh"
  },
  "dependencies": {
    "@discordjs/rest": "^2.2.0",
    "axios": "^1.6.7",
    "cron": "^3.1.6",
    "dayjs": "^1.11.10",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "fast-glob": "^3.3.2",
    "git-rev-sync": "^3.0.2",
    "lodash-es": "^4.17.21",
    "lru-cache": "^10.2.0",
    "pinia": "^2.1.7",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@types/git-rev-sync": "^2.0.2",
    "@types/lodash-es": "^4.17.12",
    "cross-env": "^7.0.3",
    "unplugin-auto-import": "^0.17.5",
    "vite-node": "^1.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
