{
  "name": "MixCat",
  "description": "A Discord bot that mixed Node.js with Vite, Vue, and Pinia",
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Proladon/startcord.git"
  },
  "keywords": [
    "discordjs",
    "discord",
    "bot",
    "vue",
    "vite"
  ],
  "author": "Proladon",
  "scripts": {
    "dev": "cross-env MODE=development vite-node --watch src/main.ts ",
    "start": "cross-env MODE=production vite-node src/main.ts ",
    "docker:build": "cross-env MODE=production sudo ./script/build_docker.sh",
    "docker:run": "sudo ./script/run_docker.sh"
  },
  "dependencies": {
    "@discordjs/rest": "^2.4.2",
    "axios": "^1.7.9",
    "cron": "^3.5.0",
    "dayjs": "^1.11.13",
    "discord.js": "^14.17.3",
    "dotenv": "^16.4.7",
    "fast-glob": "^3.3.3",
    "git-rev-sync": "^3.0.2",
    "lodash-es": "^4.17.21",
    "lru-cache": "^11.0.2",
    "pinia": "^2.3.1",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@types/git-rev-sync": "^2.0.2",
    "@types/lodash-es": "^4.17.12",
    "cross-env": "^7.0.3",
    "unplugin-auto-import": "^19.0.0",
    "vite-node": "^3.0.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}